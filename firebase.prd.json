{
  "hosting": {
    "public": "dist",
    "site": "sophon-web-prd",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/restaking",
        "destination": "/restaking.html"
      },
      {
        "source": "/marketplace",
        "destination": "/marketplace.html"
      },
      {
        "source": "/ecosystem",
        "destination": "/ecosystem.html"
      }
    ]
  }
}
